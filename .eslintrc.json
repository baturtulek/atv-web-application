{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-console": 0,
      "consistent-return": 0,
      "arrow-body-style": 0,
      "no-use-before-define": 0,
      "eqeqeq": 0,
      "radix": 0,
      "no-restricted-syntax": 0
    }
}