

<div class="container-contact100">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form" action="add" method="POST">
				<span class="contact100-form-title">
					Otopark Ekle
				</span>

				<div class="wrap-input100 validate-input bg1" data-validate="Please Type Your Name">
					<input class="input100" type="text" name="description" placeholder="Otopark Adi">
				</div>

				{{!-- <div class="wrap-input100 validate-input bg1 " data-validate = "Enter Your Email (e@a.x)">
					<input class="input100" type="text" name="email" placeholder="Açıklama ">
				</div> --}}
				<div class="wrap-input100 input100-select bg1">
					<span class="label-input100">Otopark Tipi *</span>
					<div>
						<select class="js-select2" name="parkingTypeId">
							{{#each parkingLotType}}
                <option value="{{this.id}}">{{this.description}}</option>
              {{/each}}
						</select>
					</div>
				</div>
        	<div class="wrap-input100 input100-select bg1">
					<span class="label-input100">Otopark Tipi *</span>
					<div>
						<select class="js-select2" name="staffId">
							{{#each parkingLotUsers}}
                <option value="{{this.id}}">{{this.name}} {{this.surname}}</option>
              {{/each}}
						</select>
					</div>
				</div>

				<div class="container-contact100-form-btn">
					<button class="contact100-form-btn">
						<span>
							Submit
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</button>
				</div>
			</form>
		</div>
	</div>